{
  "name": "idonthavespotify",
  "version": "1.6.4",
  "scripts": {
    "dev": "concurrently \"bun run watch:assets\" \"bun run watch:server\"",
    "watch:assets": "bun run build.config.ts --watch",
    "watch:server": "bun run --watch src/index.ts",
    "watch:js": "bun build ./src/views/controllers/index.js --outdir ./public/assets --watch",
    "watch:css": "bunx @tailwindcss/cli -i ./src/views/css/index.css -o ./public/assets/index.min.css --watch",
    "build:dev": "bun run build.config.ts --watch",
    "build": "bun run build.config.ts --minify",
    "test": "NODE_ENV=test bun test",
    "test:ci": "NODE_ENV=test bun test integration",
    "lint": "eslint --fix",
    "tscheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hotwired/stimulus": "^3.2.2",
    "@umami/node": "^0.4.0",
    "axios": "^1.7.9",
    "axios-retry": "^4.5.0",
    "cheerio": "^1.0.0",
    "howler": "^2.2.4",
    "nano-jsx": "^0.1.0",
    "notyf": "^3.10.0",
    "pino": "^8.20.0",
    "pino-pretty": "^11.0.0",
    "string-similarity": "^4.0.4",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.0.0",
    "@tailwindcss/postcss": "^4.0.0",
    "@types/bun": "^1.2.5",
    "@types/howler": "^2.2.12",
    "@types/string-similarity": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "axios-mock-adapter": "^1.22.0",
    "concurrently": "^8.2.2",
    "domhandler": "^5.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx": "^0.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.4.5"
  }
}
